<body>
<a href="#" class="scrollToTop"><i class="fa fa-angle-up"></i></a>

<!--Loader-->
<div class="loader">
	<div class="spinner centered">
		<div class="spinner-container container1">
			<div class="circle1"></div>
			<div class="circle2"></div>
			<div class="circle3"></div>
			<div class="circle4"></div>
		</div>
		<div class="spinner-container container2">
			<div class="circle1"></div>
			<div class="circle2"></div>
			<div class="circle3"></div>
			<div class="circle4"></div>
		</div>
	</div>
</div>

<!--Header-->
<header>
	{%  if app.request.attributes.get('_route') == 'home' %}
	<nav class="navbar navbar-default navbar-fixed white no-background bootsnav pushy">
	{% else %}
	<nav class="navbar navbar-default navbar-sticky bootsnav">
	{% endif %}
		<div class="container">
			<div class="navbar-header">
				<a class="navbar-brand" href="/">
					{%  if app.request.attributes.get('_route') == 'home' %}
					<img src="{{ asset('assets/images/logo-white.png') }}" alt="logo" class="logo logo-display">
					{%  endif %}
					<img src="{{ asset('assets/images/logo.png') }}" alt="logo" class="logo logo-scrolled">
				</a>
			</div>
			<div class="navbar navbar-btn" id="navbar-menu">
				<ul class="nav">
					{% if is_granted("ROLE_ADMIN") %}
						<li class="dropdown clearfix">
							<a href="#" class="dropdown">Gestion <i class="fas fa-angle-down"></i></a>
							<ul class="dropdown-menu">
								<li><a href="{{ path('promotion_index') }}">Promotions</a></li>
								<li><a href="{{ path('teacher_index') }}">Intervenants</a></li>
								<li><a href="{{ path('bde_index') }}">BDE</a></li>
							</ul>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="{{ path('user_index') }}">Utilisateurs</a>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="{{ path('subject_index') }}">Unités d'enseignement</a>
						</li>

					{% elseif is_granted("ROLE_SPEAKER") %}
						<li class="nav-item">
							<a class="nav-link" href="#">Événements</a>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="{{ path('project_index') }}">Projets</a>
						</li>
					{% elseif is_granted("ROLE_STUDENT") %}
						{% if is_granted("ROLE_DELEGATE") %}
							<li class="nav-item">
								<a class="nav-link" href="#">Événements</a>
							</li>

							<li class="nav-item">
								<a class="nav-link" href="{{ path('article_index') }}">Articles</a>
							</li>
						{% elseif is_granted("ROLE_BDE") %}
							<li class="nav-item">
								<a class="nav-link" href="{{ path('article_index') }}">Articles</a>
							</li>
						{% endif %}

						<li class="nav-item">
							<a class="nav-link" href="{{ path('user_show', {'id': app.user.id}) }}">Compte</a>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="{{ path('project_index') }}">Projets</a>
						</li>
					{% else %}
						<li class="nav-item">
							<a class="nav-link" href="#">Présentation de la formation</a>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="#">Voir les CV</a>
						</li>

						<li class="nav-item">
							<a class="nav-link" href="{{ path('project_index') }}">Voir les projets</a>
						</li>
					{% endif %}

					{% if not app.user %}
						<li class="nav-item">
							<a class="nav-link" href="{{ path('app_login') }}"><i class="fas fa-sign-in-alt"></i> Se connecter</a>
						</li>
					{% else %}
						<li class="nav-item">
							<a class="nav-link" href="{{ path('app_logout') }}"><i class="fas fa-sign-out-alt"></i> Se déconnecter</a>
						</li>
					{% endif %}

				</ul>
			</div>
		</div>
	</nav>
</header>



