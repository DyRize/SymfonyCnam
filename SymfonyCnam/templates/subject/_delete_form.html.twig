<form method="post" action="{{ path('subject_delete', {'id': subject.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ subject.id) }}">
    <button class="btn_common btn_border margin10 border_radius">Supprimer</button>
</form>
